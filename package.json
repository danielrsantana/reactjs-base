{
  "name": "flex-paayment-mercury",
  "version": "0.1.0",
  "description": "Flex Payment Mercury App",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "sass"
  ],
  "author": "Flex Payment",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/flexpaymentdev/mercury.git"
  },
  "homepage": "https://flexpayment.atlassian.net/wiki/spaces/MER/overview",
  "scripts": {
    "build": "webpack -p --config=config/webpack/dev.webpack.config.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=config/webpack/dev.webpack.config.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@types/enzyme": "^3.1.16",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "classnames": "^2.2.6",
    "immutability-helper": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "bulma": "^0.7.2",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.0.0",
    "node-sass": "^4.11.0",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "shortid": "^2.2.14",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/unitTesting/shim.unitTesting.js",
      "<rootDir>/config/unitTesting/setup.unitTesting.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>//config/unitTesting/preprocessor.unitTesting.js"
    },
    "testMatch": [
      "/*.spec.(ts|tsx|js)"
    ]
  }
}
